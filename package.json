{
  "name": "webpconvert",
  "type": "module",
  "version": "2.0.0",
  "description": "CLI Converter from png/jpg images to webp",
  "exports": "./index.js",
  "bin": "./index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
