    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-rename": "^2.0.0",
    "imagemin": "^8.0.0",
    "imagemin-webp": "^6.0.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "directory-tree": "^2.2.9",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
